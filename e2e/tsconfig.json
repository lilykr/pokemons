/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "outDir": "../out-tsc/e2e",
    "module": "commonjs",
    "target": "es2018",
    "moduleResolution": "node",
    "types": [
      "jasmine",
      "node",
    ],
    "lib": [
      "es2017"
    ],
    "strict": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "baseUrl": "."
  }
}
